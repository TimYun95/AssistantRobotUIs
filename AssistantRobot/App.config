<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>

  <appSettings>
    <!--COM连接-->
    <add key="ifUsingSerialPort" value="False"/>
    <add key="numOfCOM" value="COM3"/>
    <!--机器人配置-->
    <add key="currentRobotType" value="CBUR3"/>
    <add key="currentRobotProgramType" value="SW34"/>
    <add key="robotControllerIP" value="192.168.1.5"/>
    <add key="robotConnectorIP" value="192.168.1.7"/>
    <!--传感器配置-->
    <add key="currentSensorType" value="OldOptoForce"/>
    <add key="forceSensorIP" value="192.168.1.9"/>
    <add key="forceConnectorIP" value="192.168.1.11"/>
    <add key="ifUsingForceSensor" value="True"/>
    <!--机器人连接配置-->
    <add key="timeOutDurationMS" value="200"/>
    <add key="ifProlongTimeOutDurationWhenConnectionBegin" value="True"/>
    <add key="autoCheckingConnectableDurationMS" value="1000"/>
    <!--电流超限配置-->
    <add key="ifEnableCurrentOverFlowProtect" value="True"/>
    <add key="currentOverFlowBoundValue" value="2.0"/>
    <!--力/力矩超限配置-->
    <add key="ifEnableForceOverFlowProtect" value="True"/>
    <add key="forceOverFlowBoundValue" value="100.0"/>
    <add key="torqueOverFlowBoundValue" value="15.0"/>
    <!--工具IO配置-->
    <add key="ifEnableToolIO" value="False"/>
    <add key="digitalIOVoltage" value="0"/>
    <!--标定相关配置-->
    <add key="probeCalibrationMaxAmplitudeDeg" value="60.0"/>
    <add key="punctureUsingAttitudeFlag" value="0"/>
    <!--工具末端移动参数-->
    <add key="fastSpeedL" value="0.2"/>
    <add key="slowSpeedL" value="0.1"/>
    <add key="minSpeedL" value="0.00002"/>
    <add key="fastAccelerationL" value="0.2"/>
    <add key="slowAccelerationL" value="0.1"/>
    <add key="minAccelerationL" value="0.001"/>
    <!--关节角度移动参数-->
    <add key="fastSpeedj" value="0.4"/>
    <add key="slowSpeedj" value="0.2"/>
    <add key="minSpeedj" value="0.0004"/>
    <add key="fastAccelerationj" value="0.4"/>
    <add key="slowAccelerationj" value="0.2"/>
    <add key="minAccelerationj" value="0.002"/>
    <!--弹窗尺寸配置-->
    <add key="titleSize" value="18"/>
    <add key="messageSize" value="22"/>
  </appSettings>
</configuration>
