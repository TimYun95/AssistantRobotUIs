<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--ViewModel参数-->
    <!--工具末端移动参数-->
    <add key="fastSpeedL" value="0.2" />
    <add key="slowSpeedL" value="0.1" />
    <add key="minSpeedL" value="0.00002" />
    <add key="fastAccelerationL" value="0.2" />
    <add key="slowAccelerationL" value="0.1" />
    <add key="minAccelerationL" value="0.001" />
    <!--关节角度移动参数-->
    <add key="fastSpeedj" value="0.4" />
    <add key="slowSpeedj" value="0.2" />
    <add key="minSpeedj" value="0.0004" />
    <add key="fastAccelerationj" value="0.4" />
    <add key="slowAccelerationj" value="0.2" />
    <add key="minAccelerationj" value="0.002" />
    <!--弹窗尺寸配置-->
    <add key="titleSize" value="18"/>
    <add key="messageSize" value="22"/>
    
    <!--CommunicationModel参数-->
    <!--网段配置-->
    <add key="ifAtSamePC" value="False"/>
    <add key="ifAtSameLAN" value="True"/>
    <add key="netAdapterName" value="以太网"/>
    <!--服务器IP地址配置-->
    <add key="serverIPAtSameLAN" value="192.168.1.13"/>
    <add key="serverIPAtWAN" value="202.120.48.24"/>
    <!--设备号配置-->
    <add key="clientDeviceIndex" value="1"/>
    <!--TCP时间配置-->
    <add key="tcpSocketSendTimeOut" value="1000"/>
    <add key="tcpSocketRecieveTimeOut" value="2000"/>
    <add key="tcpBeatInterval" value="1000"/>
    <!--缓冲区大小配置-->
    <add key="maxBufferSize" value="20"/>
    <!--队列间隔时间配置-->
    <add key="waitTimerMsForBuffer" value="30"/>
  <!--启动监控程序路径配置-->
    <add key="superviseProgramName" value="AssistantRobotRemoteSupervisor.exe"/>
    <add key="superviseProgramPath" value="D:\SY\Codes\URLastPrj\AssistantRobotRemoteSupervisor\AssistantRobotRemoteSupervisor\bin\x64\Release\"/>
  
  </appSettings>

  
</configuration>